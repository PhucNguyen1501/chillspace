{
  "name": "api-doc-query-builder",
  "version": "1.0.0",
  "description": "Chrome extension to read API docs, convert natural language to API queries, and schedule data extraction",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "clsx": "^2.0.0",
    "exceljs": "^4.4.0",
    "js-yaml": "^4.1.0",
    "lucide-react": "^0.294.0",
    "papaparse": "^5.4.1",
    "prism-react-renderer": "^2.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/chrome": "^0.0.254",
    "@types/js-yaml": "^4.0.9",
    "@types/papaparse": "^5.3.14",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/ui": "^4.0.8",
    "autoprefixer": "^10.4.16",
    "happy-dom": "^20.0.10",
    "jsdom": "^27.2.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.3",
    "vite": "^7.2.2",
    "vitest": "^4.0.8"
  }
}
